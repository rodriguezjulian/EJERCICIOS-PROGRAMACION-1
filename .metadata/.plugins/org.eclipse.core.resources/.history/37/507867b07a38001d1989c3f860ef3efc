#include <stdio.h>
#include <stdlib.h>
void validarEntradaDeOpcion(int flagCostos,int flagJugadores)
{

	if(flagCostos==1 && flagJugadores==1)
	{
		printf("\nCalculos realizados correctamente\n");
	}else
	{
		printf("\nERROR, Primero debe cargar costos e ingresar jugadores\n");
	}
}

