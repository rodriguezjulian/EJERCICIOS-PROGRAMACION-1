//RODRIGUEZ JULIAN MANUEL DIV J - EJERCICIO ARRAYS EN CLASE
#include <stdio.h>
#include <stdlib.h>
#define TAM 5

int calcularPromedioArray(int tam, int* array, int *promedio);

int main(void) {

	setbuf(stdout,NULL);

	int numerosIngresados[TAM];
	int i;
	int acumulador;
	acumulador=0;

	numerosIngresados[TAM]= ingresarInt();

	for(i=0;i<5;i++)
	{
		acumulador=acumulador+array;
	}

	return EXIT_SUCCESS;
}
int calcularPromedioArray(int tam, int* array, int *promedio)
{
	int retorno=-1;
	int acumulador;
	acumulador=0;
	if(tam>0 && array!=NULL && promedio!=NULL)
	{
		for(int i=0;i<5;i++)
		{
			acumulador=acumulador+array[i];
		}
		retorno=0;
	}
	return retorno;
}









