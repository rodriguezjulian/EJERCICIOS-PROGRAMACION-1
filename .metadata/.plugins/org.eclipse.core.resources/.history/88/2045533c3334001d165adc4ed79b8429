#include <stdio.h>
#include <stdlib.h>
#include "ingresos.h"
#define TAM 5

int calcularMaximoArray(int* array, int* valorMaximo);

int main(void) {
	setbuf(stdout,NULL);
	int numerosIngresados[TAM];

	for(int i=0;i<TAM;i++)
	{
		numerosIngresados[i]= ingresarInt();
	}

	return EXIT_SUCCESS;
}
int calcularMaximoArray(int* array, int* valorMaximo)
{
	*valorMaximo=array[0];
	for(int i=0;i<5;i++)
	{
		for(int i=0;i<5;i++)
		{
			if(valorMaximo>array[i])
			{
				*valorMaximo=array[i];
			}
		}

	}

}
